<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%date{"yyyy-MM-dd'T'HH:mm:ss.SSSXXX", UTC} %-5level [%thread] %logger: %msg%n%rootException</pattern>
    </encoder>
  </appender>
  <root level="ALL">
    <appender-ref ref="STDOUT"/>
  </root>
</configuration>
